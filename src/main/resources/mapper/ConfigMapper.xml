<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.firstdemo.Mapper.ConfigMapper">

    <resultMap id="BaseResultMap" type="com.example.Chat.Entity.Config">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="configkey" column="configKey" jdbcType="VARCHAR"/>
            <result property="configvalue" column="configValue" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,configKey,configValue
    </sql>
</mapper>
